#include"stdio.h"
void swap(int*,int*);
main()
{
	int a,b,x;

	printf("Enter the two numbers: ");
	scanf("%d %d",&a,&b);
	
	if(a < b)
	{
		printf("%d is smaller than %d",a,b);
	}
	else if(a > b)
	{
		swap(&a,&b);
		printf("%d is greater than %d",b,a);
	}
	else
		printf("Both are same\n");
}
void swap(int *a,int *b)
{
	int temp;
	temp=*a;
	*a=*b;
	*b=temp;
}
