#include"stdio.h"
#include"stdlib.h"
main()
{
	int i,j,n,c,*ptr;
	int count,sum=0,t=0;

	printf("Enter the size of the array: ");
	scanf("%d",&n);

	ptr=(int*)malloc(n*sizeof(int));

	printf("Enter elements of array: ");
	for(i=0;i<n;i++)
	{
		scanf("%d",&ptr[i]);
	}

	int ele[1000];

	for(i=0;i<=n;i++)
	{
		count=0;
		for(j=0;j<=ptr[i];j++)
		{
			if(ptr[i]%(j+1)==0)
				count++;
		}
		
		if(count == 2)
		{
			ele[t]=ptr[i];
			t++;		
		}	
	}

	for(t=0;t<n;t++)
	{
		sum += ele[t];		
	}

	printf("SUM=%d",sum);

	free(ptr);
}
